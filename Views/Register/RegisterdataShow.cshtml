
@model List<Register>
<div class="container pt-4">
	<table class="table table-bordered table-striped" >
		<thead>
	<tr>
		<th>Name</th>
		<th>Email</th>
	     <th>Password</th>
		 <th>gender</th>
		 <th>Address</th>
		 <th>PhoneNumber</th>
		 <th>DateOfBirth</th>
		 
	</tr>
	</thead>

	<tbody>
	@foreach (var Row in Model)
{
	<tr>
		<td>@Row.Name</td>
		<td>@Row.Email</td>
		<td>@Row.Password</td>
		<td>@Row.gender</td>
		<td>@Row.Address</td> 
		<td>@Row.PhoneNumber</td>
		<td>@Row.DateOfBirth</td>
					@* <td><input type="button" value="edit" name=@Row.Id /></td> *@
					<td><a asp-controller="Register" asp-action="Edit" asp-route-id="@Row.Id" class="btn btn-primary">Edit</a></td>
					<td><a asp-controller="Register" asp-action="Delete" asp-route-id="@Row.Id" class="btn btn-primary">Delete</a></td>
	</tr>

	}
	</tbody>
	
</table>

</div>

<div class="container">
	<div class="row pt-3">
		<div class="col-6 col-md-3">
			<a asp-controller="Register" asp-action="gettinguserdata"
			   class="btn btn-primary border form-control">Add data to the Register</a>
		</div>

	</div>
</div>





